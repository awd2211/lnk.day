{
  "name": "@lnk/user-service",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "nest build",
    "dev": "nest start --watch",
    "start": "nest start",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "test": "jest",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:generate": "npm run build && npm run typeorm -- -d ./dist/database/data-source.js migration:generate",
    "migration:create": "npm run typeorm -- migration:create",
    "migration:run": "npm run build && npm run typeorm -- -d ./dist/database/data-source.js migration:run",
    "migration:revert": "npm run build && npm run typeorm -- -d ./dist/database/data-source.js migration:revert",
    "migration:show": "npm run build && npm run typeorm -- -d ./dist/database/data-source.js migration:show"
  },
  "dependencies": {
    "@lnk/nestjs-common": "workspace:*",
    "@lnk/shared-types": "workspace:*",
    "@nestjs-modules/ioredis": "^2.0.2",
    "@nestjs/common": "^10.3.0",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.3.0",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.3.0",
    "@nestjs/schedule": "^4.0.0",
    "@nestjs/swagger": "^7.1.17",
    "@nestjs/throttler": "^5.2.0",
    "@nestjs/typeorm": "^10.0.1",
    "axios": "^1.6.2",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "ioredis": "^5.3.2",
    "ldapjs": "^3.0.7",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.1.14",
    "rxjs": "^7.8.1",
    "samlify": "^2.8.11",
    "stripe": "^14.10.0",
    "typeorm": "^0.3.17",
    "xml-crypto": "^3.2.0",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "@lnk/eslint-config": "workspace:*",
    "@lnk/tsconfig": "workspace:*",
    "@nestjs/cli": "^10.2.1",
    "@nestjs/schematics": "^10.0.3",
    "@nestjs/testing": "^10.3.0",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/ldapjs": "^3.0.6",
    "@types/node": "^20.10.5",
    "@types/passport-jwt": "^4.0.0",
    "@types/xml2js": "^0.4.14",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
