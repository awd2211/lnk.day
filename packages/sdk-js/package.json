{
  "name": "@lnk/sdk",
  "version": "1.0.0",
  "description": "Official JavaScript/TypeScript SDK for lnk.day link management platform",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "lint": "eslint src --ext .ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "lnk",
    "url-shortener",
    "link-management",
    "analytics",
    "sdk"
  ],
  "author": "lnk.day",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/lnkday/sdk-js"
  },
  "dependencies": {
    "axios": "^1.6.2"
  },
  "devDependencies": {
    "@lnk/eslint-config": "workspace:*",
    "@lnk/tsconfig": "workspace:*",
    "@types/node": "^20.10.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.2",
    "vitest": "^1.0.4"
  },
  "peerDependencies": {
    "typescript": ">=4.7.0"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
